  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsc --build",
    "compile": "hardhat compile",
    "deploy:localhost": "hardhat deploy --network localhost --export-all deployments/networks.json",
    "mint:localhost": "hardhat mint --network localhost",
    "mint:maticmum": "hardhat mint --network maticmum",
    "mint:matic": "hardhat mint --network matic",
    "deploy:hardhat": "hardhat deploy --network hardhat --export-all deployments/networks.json",
    "deploy:maticmum": "hardhat deploy --network maticmum --export-all deployments/networks.json",
    "deploy:matic": "hardhat deploy --network matic --export-all deployments/networks.json",
    "deploy:goerli": "hardhat deploy --network goerli --export-all deployments/networks.json",
    "deploy:rinkeby": "hardhat deploy --network rinkeby --export-all deployments/networks.json",
    "start:dev": "hardhat node --network hardhat  --export-all deployments/networks.json",
    "test": "hardhat test",
    "prepublish": "yarn compile && yarn build && cp contracts/* ."
  },
  "dependencies": {
    "@divergencetech/ethier": "^0.27.0",
    "erc721a": "^3.2.0",
